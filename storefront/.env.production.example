# Production Environment Variables for Storefront
# Copy this file to .env.production and fill in your actual values

# Medusa Backend URL (your VPS backend domain)
NEXT_PUBLIC_MEDUSA_BACKEND_URL=https://your-backend-domain.com

# Base URL for the storefront (your VPS storefront domain)
NEXT_PUBLIC_BASE_URL=https://your-storefront-domain.com

# Revalidation time (in seconds)
REVALIDATE_TIME=3600
